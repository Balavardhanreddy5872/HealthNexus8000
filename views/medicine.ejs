                  <td><a href="#S">S</a></td> <td><a href="#U">U</a></td> <td><a href="#V">V</a></td> <td><a href="#W">W</a></td><td id="all">ALL</td>
              </tr>
          </thead>
       </table>
       </div>
<!---Med head end-->

    <!-- image section -->
    <h2 class="product-category">Alphabetical Order</h2>
    <% for(let i=0;i<list.length;i++) { %> 
    <section class="product"> 
      <h2 class="product-category left-alp" id="<%=String.fromCharCode('A'.charCodeAt(0)+i)%>"><%=String.fromCharCode('A'.charCodeAt(0)+i)%></h2>
      <button class="pre-btn"><img src="images/arrow.png" alt=""></button>
      <button class="nxt-btn"><img src="images/arrow.png" alt=""></button>

    
      <div class="product-container">
      <%  for(let j=0;j<list[i].length;j++) { %>
      <form action="/cart" method="get">
        <div class="product-card">
          <div class="product-image">
            <span class="discount-tag">50% off</span>
            <img src="<%= list[i][j].image %>" class="product-thumb" alt="">
          </div>
          <div class="product-info">
            <h2 class="product-brand"><input type="hidden" name="name" value="<%= list[i][j].name %>"><%=  list[i][j].name %></h2>
            <!-- <p class="product-short-description">Atrest 12.5mg Tablet 10'S</p> -->
            <input type="hidden" name="price" value="<%= list[i][j].price1 %>">
            <span class="price">&#8377 <%= list[i][j].price1 %></span><span class="actual-price">&#8377 <%= list[i][j].price2 %></span>
          </div>
            <button type="submit">Add to Cart</button>
      </div>
      </form>
      
        <% } %>
      </div>
</section>
<% } %>
