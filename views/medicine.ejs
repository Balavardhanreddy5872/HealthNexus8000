    <div id="MedPage">
      <div id="info">
        <p class="cat">Find Your Prescription Medicines</p>
          <h1 class="h4">Shop by Drug Category</h1>
          <br>
          <table>
           <thead>
              <tr>
                  <td><a href="#A">A</a></td> <td><a href="#B">B</a></td> <td><a href="#C">C</a></td> 
                  <td><a href="#D">D</a></td> 
                  <td><a href="#E">E</a></td><td><a href="#F">F</a></td><td><a href="#G">G</a></td><td><a href="#H">H</a></td><td><a href="#I">I</a></td>
